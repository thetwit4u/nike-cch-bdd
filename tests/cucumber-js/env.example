# AWS configuration
AWS_REGION=eu-west-1
# Optional: rely on default credentials provider chain (env/SSO/profile). Set if needed:
# AWS_PROFILE=dev-bdd-private

# Orchestrator command queue (SQS URL)
COMMAND_QUEUE_URL=https://sqs.eu-west-1.amazonaws.com/123456789012/cch-orchestrator-command.fifo

# Flow Controller start queue (SQS URL) used to kick off the orchestrator from ASN
CONTROLLER_QUEUE_URL=https://sqs.eu-west-1.amazonaws.com/123456789012/cch-controller-start.fifo

# CCH System Events SNS topic (ARN). The harness will create a temp SQS queue and subscribe it.
SYSTEM_EVENTS_TOPIC_ARN=arn:aws:sns:eu-west-1:123456789012:cch-system-events

# S3 bucket used to upload workflow definitions and consignment data.
# If not set, the harness will automatically create a temporary bucket for the run and delete it at the end.
# START_S3_BUCKET=
START_S3_KEY_PREFIX=workflows/

# Tag for correlation naming and interpolation
TEST_USER=dave

# Timeouts (seconds)
SCENARIO_TIMEOUT_SECONDS=120

